{
  "name": "vite-project",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run generate:sitemap && npm run generate:icons",
    "preview": "vite preview",
    "deploy:check": "node scripts/deploy-check.js",
    "deploy:vercel": "npm run deploy:check && vercel --prod",
    "deploy:netlify": "npm run deploy:check && netlify deploy --prod",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:icons": "node scripts/generate-icons.js",
    "seo:analyze": "node scripts/seo-analyze.js",
    "seo:audit": "lighthouse --output=html --output-path=./reports/lighthouse.html --view",
    "performance:test": "node scripts/performance-test.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "element-plus": "^2.10.7",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@vitejs/plugin-vue": "^6.0.1",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}